    "inputs",
    "validation",
    "hoc",
    "enhancer",
    "simple",
    "easy"
  ],
  "author": "Artur Muller <me@arturmuller.com> (http://arturmuller.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arturmuller/react-informal/issues"
  },
  "homepage": "https://github.com/arturmuller/react-informal#readme",
  "dependencies": {
    "debug": "^2.6.3",
    "invariant": "^2.2.2",
    "prop-types": "^15.5.8",
    "ramda": "^0.23.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-lusk": "0.0.2",
    "eslint": "^4.4.1",
    "eslint-config-lusk": "^2.0.0",
    "prettier-eslint-cli": "^4.1.1"
  }
