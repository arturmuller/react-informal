{
  "name": "react-informal",
  "version": "0.1.0",
  "description": "React forms without the ceremony",
  "main": "cjs/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "babel ./lib --out-dir ./cjs",
    "dev": "babel ./lib --out-dir ./cjs --watch",
    "format": "prettier-eslint './lib/**/*.js' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arturmuller/react-informal.git"
  },
  "keywords": [
    "react",
    "form",
    "forms",
